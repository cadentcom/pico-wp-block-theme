{
	"version": 2,
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#3e8cff",
					"name": "Pico Primary color",
					"slug": "custom-pico-primary-color"
				},
				{
					"color": "#f3faff",
					"name": "Pico secondary color",
					"slug": "custom-pico-secondary-color"
				},
				{
					"color": "#07306e",
					"name": "Pico navy blue color",
					"slug": "custom-pico-navy-blue-color"
				},
				{
					"color": "#141414",
					"name": "Pico heading color",
					"slug": "custom-pico-heading-color"
				},
				{
					"color": "#555555",
					"name": "Pico body text color",
					"slug": "custom-pico-body-text-color"
				}
			]
		},
		"layout": {
			"contentSize": "1140px",
			"wideSize": "1366px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/poppins_normal_300.ttf"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/poppins_normal_400.ttf"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/poppins_normal_500.ttf"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/poppins_normal_600.ttf"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/poppins_normal_700.ttf"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "800",
							"src": [
								"file:./assets/fonts/poppins_normal_800.ttf"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "900",
							"src": [
								"file:./assets/fonts/poppins_normal_900.ttf"
							]
						}
					],
					"fontFamily": "Poppins",
					"slug": "poppins"
				},
				{
					"fontFace": [
						{
							"fontFamily": "DM Sans",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/dm-sans_normal_300.ttf"
							]
						},
						{
							"fontFamily": "DM Sans",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/dm-sans_normal_400.ttf"
							]
						},
						{
							"fontFamily": "DM Sans",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/dm-sans_normal_500.ttf"
							]
						},
						{
							"fontFamily": "DM Sans",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/dm-sans_normal_600.ttf"
							]
						},
						{
							"fontFamily": "DM Sans",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/dm-sans_normal_700.ttf"
							]
						},
						{
							"fontFamily": "DM Sans",
							"fontStyle": "normal",
							"fontWeight": "800",
							"src": [
								"file:./assets/fonts/dm-sans_normal_800.ttf"
							]
						},
						{
							"fontFamily": "DM Sans",
							"fontStyle": "normal",
							"fontWeight": "900",
							"src": [
								"file:./assets/fonts/dm-sans_normal_900.ttf"
							]
						}
					],
					"fontFamily": "DM Sans",
					"slug": "dm-sans"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"color": {
					"background": "var(--wp--preset--color--custom-pico-primary-color)",
					"text": "var(--wp--preset--color--custom-pico-secondary-color)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--custom-pico-secondary-color)"
						}
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "16px",
					"fontStyle": "normal",
					"fontWeight": "400",
					"textTransform": "capitalize"
				},
				"variations": {
					"outline": {
						"border": {
							"bottom": {
								"color": "#3e8cff",
								"style": "solid",
								"width": "1px"
							},
							"color": "#3e8cff",
							"left": {
								"color": "#3e8cff",
								"style": "solid",
								"width": "1px"
							},
							"radius": "100px",
							"right": {
								"color": "#3e8cff",
								"style": "solid",
								"width": "1px"
							},
							"style": "solid",
							"top": {
								"color": "#3e8cff",
								"style": "solid",
								"width": "1px"
							},
							"width": "1px"
						},
						"color": {
							"background": "var(--wp--preset--color--custom-pico-primary-color)",
							"text": "var(--wp--preset--color--custom-pico-secondary-color)"
						}
					}
				}
			},
			"core/columns": {
				"spacing": {
					"margin": {
						"bottom": "0px",
						"top": "0px"
					}
				}
			},
			"core/group": {
				"spacing": {
					"margin": {
						"bottom": "0px",
						"top": "0px"
					}
				}
			},
			"core/heading": {
				"color": {
					"text": "var(--wp--preset--color--custom-pico-heading-color)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--custom-pico-heading-color)"
						}
					}
				},
				"spacing": {
					"margin": {
						"bottom": "0px",
						"top": "0px"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontStyle": "normal",
					"fontWeight": "700",
					"lineHeight": "1.5"
				}
			},
			"core/list": {
				"spacing": {
					"margin": {
						"bottom": "0px",
						"left": "0px",
						"right": "0px",
						"top": "0px"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--dm-sans)",
					"fontSize": "18px",
					"lineHeight": "1.6"
				}
			},
			"core/navigation": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--50)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--dm-sans)",
					"fontSize": "16px",
					"fontStyle": "normal",
					"fontWeight": "400",
					"textTransform": "capitalize"
				}
			},
			"core/paragraph": {
				"color": {
					"text": "var(--wp--preset--color--custom-pico-body-text-color)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--custom-pico-body-text-color)"
						}
					}
				},
				"spacing": {
					"margin": {
						"bottom": "0px",
						"top": "0px"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--dm-sans)",
					"fontSize": "18px",
					"fontStyle": "normal",
					"fontWeight": "400",
					"lineHeight": "1.6"
				}
			},
			"core/post-title": {
				"spacing": {
					"margin": {
						"bottom": "0px",
						"top": "0px"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontStyle": "normal",
					"fontWeight": "700"
				}
			},
			"core/quote": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontStyle": "normal",
					"fontWeight": "400"
				}
			},
			"core/site-tagline": {
				"spacing": {
					"margin": {
						"bottom": "0px",
						"top": "0px"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--dm-sans)",
					"fontSize": "16px",
					"fontStyle": "normal",
					"fontWeight": "400",
					"textTransform": "capitalize"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "30px",
					"fontStyle": "normal",
					"fontWeight": "600"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--custom-pico-secondary-color)",
			"text": "var(--wp--preset--color--custom-pico-body-text-color)"
		},
		"elements": {
			"button": {
				"color": {
					"text": "var(--wp--preset--color--custom-pico-primary-color)"
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--custom-pico-body-text-color)"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--custom-pico-heading-color)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontStyle": "normal",
					"fontWeight": "700",
					"lineHeight": "1.4"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--custom-pico-heading-color)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"textDecoration": "none"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--dm-sans)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	]
}